{
    "name": "schematics",
    "version": "0.0.0",
    "description": "A blank schematics",
    "scripts": {
        "clean": "rimraf dist/",
        "copy-files": "copyfiles -u 1 src/**/*.json src/**/files/**/* src/**/files/**/.vscode/* src/**/files/**/.* dist/",
        "build": "tsc -p tsconfig.json",
        "build:full": "npm run clean && npm run build && npm run copy-files",
        "build:test": "npm run build && npm run copy-files",
        "test": "npm run build:full && jasmine dist/**/*.spec.js"
    },
    "keywords": [
        "schematics"
    ],
    "author": "",
    "license": "MIT",
    "schematics": "./src/collection.json",
    "dependencies": {
        "@angular-devkit/core": "^16.2.1",
        "@angular-devkit/schematics": "^16.2.1",
        "@schematics/angular": "^16.2.1",
        "typescript": "~5.1.3"
    },
    "devDependencies": {
        "@types/jasmine": "~4.3.0",
        "@types/node": "^16.11.7",
        "copyfiles": "^2.4.1",
        "jasmine": "^4.0.0",
        "rimraf": "^5.0.1"
    }
}
